@page
@model BankStartWeb.Pages.Customerpages.CustomerModel
@{
}

Customername: @Model.Givenname @Model.Surname
Streetadress: @Model.Streetaddress
City: @Model.City

Accounts: 
   
<table>
    <tr>
        <th>AccountType</th>
        <th>AccountId</th>
        <th>Balance</th>
    </tr>
       @foreach(var account in Model.Accounts)
       {
        <tr>
           <td>@account.AccountType</td>
           <td>@account.Id</td>
           <td>@account.Balance</td>
          <td><a class="ctabutton" asp-page="/CustomerPages/Transactiondetals" asp-route-accountId="@account.Id">View</a> </td>
       </tr>
        }
</table>